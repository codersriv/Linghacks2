import java.sql.Time;

public class Event {

    private String eventName;
    private Time time;
    private DateClass date;


    public Event() {}

    public Event(String eventName, Time timeOfEvent, DateClass date)
    {
        this.eventName = eventName;
        this.time = timeOfEvent;
        this.date = date;
    }

    public String getEventName() {
        return eventName;
    }

    public Time getTime() {
        return time;
    }

    public void setEventName(String eventName) {
        this.eventName = eventName;
    }

    public void setTimeOfEvent(Time timeOfEvent) {
        this.time = timeOfEvent;
    }

    public DateClass getDate() {
        return date;
    }

    public void setDate(DateClass date) {
        this.date = date;
    }

    public String toString()
    {
        return eventName + " " + date.toString() + " " + time.toString();
    }

}
